<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hero.wireless.web.dao.business.IAgentLimitDAO">
  <resultMap id="BaseResultMapExt" type="com.hero.wireless.web.entity.business.ext.AgentLimitExt" extends="BaseResultMap">
  	<result property="agentRole.id" column="r_id" jdbcType="INTEGER"/>
  </resultMap>
  
   <select id="selectBindRoleLimitByRoleId" parameterType="int" resultMap="BaseResultMapExt">
   	    SELECT a.*,b.id r_id FROM  agent_limit a LEFT JOIN agent_role_limit b ON a.Id=b.limit_id AND b.Role_Id=#{value} ORDER BY CODE asc
  </select>
</mapper>