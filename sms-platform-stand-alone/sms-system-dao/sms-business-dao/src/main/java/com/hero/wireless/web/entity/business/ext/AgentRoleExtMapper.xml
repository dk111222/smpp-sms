<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hero.wireless.web.dao.business.IAgentRoleDAO">
  <resultMap id="BaseResultMapExt" type="com.hero.wireless.web.entity.business.ext.AgentRoleExt" extends="BaseResultMap">
    <result property="agent.id" column="u_id" jdbcType="INTEGER"/>
  </resultMap>
  <select id="selectBindAgentRoleByExample" parameterType="int" resultMap="BaseResultMapExt">
        SELECT a.*,b.id u_id
        FROM  agent_role a
        LEFT JOIN agent_roles b ON a.Id=b.Role_Id AND b.Agent_Id=#{value}
  </select>
</mapper>